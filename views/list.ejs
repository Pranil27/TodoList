<%- include("header") -%>


  <body>
    <div class="box" id="heading">
      <h1><%= listTitles  %></h1>
    </div>

   <div class="box">


      <% for(var i=0;i<newListItem.length;i++){ %>
        <div class="item">
          <input type="checkbox" >
           <p><%= newListItem[i] %></p>
        </div>

      <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off" >
        <button type="submit" name="button" value="<%= listTitles %>">+</button>
      </form>

   </div>




  </body>


<%- include("footer") -%>
